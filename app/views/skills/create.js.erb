		  url: `/skills/<%= @skill.id %>`,
		  type: 'DELETE',
		  data: { 
		  "authenticity_token": "F5nAxhIi0R2zwNE77wXvOHfUsukeiVAf6O337a37i7rk2dRxyEieTL53B9MsZn6rF3OzNaS/08W99PISFdLJ4A==",
		  id: `<%= @skill.id %>`
		  },
	  });
  }
});