let $tag = $(`.tag.new:first`),
$a = $tag.children('a.skill');

$tag.addClass('old');
$a.addClass('old');

$tag.removeClass('new');
$a.removeClass('new');

$a.attr('id', 'skill<%= @skill.id %>');

$a.bind('click', function(e) {
  e.preventDefault();
  $(this).unbind('click');
  if ($a.parent('div.tag').hasClass('old')) {
