<% content_for :head_tag do %>
    <%= javascript_include_tag 'simpleform.min.js' %>
    <%= javascript_include_tag 'users/registrations.js' %>
    <%= javascript_include_tag 'validation.js' %>
<% end %>
<div class="registration">
    <center>
        <h1>Sign up</h1>
    </center>
    <div class="main">
        <div id="wrapper" class="wrapper_w3layouts">
            <%= form_for(resource, as: resource_name, url: registration_path(resource_name), html: {class: "testform", id: "testform"}) do |f| %>
            <%= render "devise/shared/error_messages", resource: resource %>
            <div class="fieldset">
                <!-- user params -->
                <fieldset class="personal-data w3_agileits_field">
                    <span class="w3l_personal">
                        <%= f.text_field :username, autofocus: true, autocomplete: "username", placeHolder:"Pseudo", class: "form-control", value: "Linx", required: "" %>
                    </span>
                    <span class="w3ls_personal">
                        <%= f.email_field :email, autofocus: true, autocomplete: "email", placeHolder:"email@address.com", class: "form-control", value: "plinsy@yopmail.com", required: "" %>
                    </span>
                    <span class="agileits_personal">
                        <%= f.password_field :password, autocomplete: "new-password", placeHolder:"******", class: "form-control", value: "000000", required: "" %>
                    </span>
                    <span class="agileits_personal">
                        <%= f.password_field :password_confirmation, autocomplete: "new-password", placeHolder:"******", class: "form-control", value: "000000", required: ""  %>
                    </span>
                </fieldset>
                <!-- end user params -->
                <fieldset class="personal-data w3_agileits_field">
                    <div class="user_status row">
                        <div class="status">
                            <div class="col-6 col-sm-6 col-md-6 col-lg-6">
                                <input class="form-control" value="student" type="radio" name="status" id="student" checked="" />
                                <label for="student">Etudiant</label>
                            </div>
                            <div class="col-6 col-sm-6 col-md-6 col-lg-6">
                                <input class="form-control" value="company" type="radio" name="status" id="company" />
                                <label for="company">Enteprise</label>
                            </div>
                        </div>
                        <span class="w3_agile_personal">
                            <select class="form-control" name="country" id="country">
                                <% CS.countries.map do |c| %>
                                <option value="<%= c[0] %>">
                                    <%= c[1] %>
                                </option>
                                <% end %>
                            </select>
                        </span>
                        <span class="agile_personal">
                            <select class="form-control" name="city" id="city" required="">
                                <option>Choose a state</option>
                                <% CS.states(:AD).map do |c| %>
                                <option value="<%= c[0] %>">
                                    <%= c[1] %>
                                </option>
                                <% end %>
                            </select>
                        </span>
                        <div class="inputs">
                            <span class="agileits_personal">
                                <input value="0347739305" class="form-control" type="tel" id="student_tel" name="student[tel]" placeholder="Telephone personal" required="" />
                            </span>
                        </div>
                    </div>
                </fieldset>
                <fieldset class="address-data-input w3_agileits_field agileinfo_field user-form">
                    <div class="row">
                        <div class="col-md-6" style="padding-right: 2%">
                            <span class="w3l_personal">
                                <label class="agileits_label" for="student_lastname">Nom de famille</label>
                                <input value="A.N.Tsimanarson" class="form-control" type="text" id="student_lastname" name="student[lastname]" placeholder="Nom de famille" required="" />
                            </span>
                        </div>
                        <div class="col-md-6" style="padding-left: 2%">
                            <span class="w3l_personal">
                                <label class="agileits_label" for="student_firstname">Prénom</label>
                                <input value="Princy" class="form-control" type="text" id="student_firstname" name="student[firstname]" placeholder="Prénom" required="" />
                            </span>
                        </div>
                    </div>
                    <div class="row">
                        <div class="col-md-6" style="padding-right: 2%">
                            <span class="w3layouts_personal">
                                <label class="agileits_label" for="student_birthdate">Date de naissance</label>
                                <input class="form-control" type="date" id="student_birthdate" name="student[birthdate]" placeholder="03X XXX XX XXX" required="" />
                            </span>
                        </div>
                        <div class="col-md-6" style="padding-left: 2%">
                            <span class="w3_agileits_personal">
                                <label class="agileits_label" for="student_address">Addresse</label>
                                <input value="Lot FMAI 124 bis 67ha SUD" class="form-control" type="text" id="student_address" name="student[address]" placeholder="Somewhere on the earth" required="" />
                            </span>
                        </div>
                    </div>
                    <div class="row">
                        <div class="col-md-6" style="padding-right: 2%">
                            <span class="w3_agileits_personal">
                                <label class="agileits_label" for="student_sex">Sexe</label>
                                <select class="form-control" name="student[sexe]" id="student_sexe">
                                    <option>Homme</option>
                                    <option>Femme</option>
                                    <option>Non confirmé</option>
                                </select>
                            </span>
                        </div>
                        <div class="col-md-6" style="padding-left: 2%">
                            <span class="w3_agileits_personal">
                                <label class="agileits_label" for="student_marital_status">état civil</label>
